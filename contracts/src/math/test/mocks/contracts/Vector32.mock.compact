pragma language_version >= 0.20.0;

import CompactStandardLibrary;

import { U256 } from "../../../types/TU256";

import "../../../Bytes8" prefix Bytes8_;

import "../../../Vector32" prefix Vector32_;

export { U256 };

// Helper for test suite: hardcoded to true in every circuit to enable circuit metadata reporting; only increases circuit size by 3 rows per exposed circuit.
ledger toImpure: Boolean;

export circuit toU256(vec: Vector<32, Uint<8>>): U256 {
  toImpure = true;
  return disclose(Vector32_toU256(vec));
}

export circuit toBytes(vec: Vector<32, Uint<8>>): Bytes<32> {
  toImpure = true;
  return Vector32_toBytes(vec);
}
