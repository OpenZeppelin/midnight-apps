{
  "name": "@openzeppelin/midnight-apps-compact-std",
  "version": "0.1.0",
  "description": "A helper Compact package for the STD library types",
  "keywords": ["compact", "std", "midnight", "starter", "contracts", "zk"],
  "author": "OpenZeppelin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenZeppelin/midnight-apps.git"
  },
  "bugs": {
    "url": "https://github.com/OpenZeppelin/midnight-apps/issues"
  },
  "homepage": "https://github.com/OpenZeppelin/midnight-apps#readme",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./artifacts": "./dist/artifacts/index.js"
  },
  "scripts": {
    "prepare": "pnpm build",
    "compact": "pnpm exec compact-compiler",
    "compact:show": "pnpm exec compact-compiler --show-circuits",
    "compact:fast": "pnpm exec compact-compiler --skip-zk",
    "build": "pnpm exec compact-builder && tsc",
    "test": "vitest run --printConsoleTrace",
    "types": "tsc -p tsconfig.json --noEmit",
    "fmt": "biome format --write",
    "lint": "biome lint",
    "lint:fix": "biome check --write",
    "precommit": "lint-staged && pnpm run types"
  },
  "packageManager": "pnpm@10.4.1",
  "devDependencies": {
    "@types/node": "^24.10.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@openzeppelin/midnight-apps-biome-config": "workspace:^",
    "@openzeppelin/midnight-apps-compact": "workspace:^",
    "@midnight-ntwrk/compact-runtime": "^0.9.0",
    "@midnight-ntwrk/midnight-js-network-id": "^2.0.1",
    "@midnight-ntwrk/zswap": "^4.0.0"
  }
}
