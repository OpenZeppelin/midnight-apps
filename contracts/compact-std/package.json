{
  "name": "@openzeppelin/midnight-apps-compact-std",
  "version": "1.0.0-alpha.1",
  "private": true,
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./artifacts": "./dist/artifacts/index.js"
  },
  "scripts": {
    "prepare": "pnpm compact:fast",
    "compact": "pnpm exec compact-compiler",
    "compact:fast": "pnpm exec compact-compiler --skip-zk",
    "build": "pnpm exec compact-builder && tsc",
    "test": "vitest run --printConsoleTrace",
    "types": "tsc -p tsconfig.json --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "lint:ci": "biome ci . --changed --since=main --no-errors-on-unmatched",
    "precommit": "lint-staged && pnpm run types"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.4.1",
  "devDependencies": {
    "@types/node": "^24.10.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@openzeppelin/midnight-apps-biome-config": "workspace:^",
    "@openzeppelin/midnight-apps-compact": "workspace:^",
    "@midnight-ntwrk/compact-runtime": "^0.9.0",
    "@midnight-ntwrk/midnight-js-network-id": "^2.0.1",
    "@midnight-ntwrk/zswap": "^4.0.0"
  }
}
