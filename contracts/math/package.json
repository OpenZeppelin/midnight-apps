{
  "name": "@midnight-dapps/math",
  "version": "1.0.0-alpha.1",
  "private": true,
  "description": "Compacts contracts math operations.",
  "type": "module",
  "exports": {
    ".": "./dist/Index.compact",
    "./interfaces/*": "./dist/interfaces/*.compact",
    "./IUint128": "./dist/interfaces/IUint128.compact",
    "./IMathU128": "./dist/interfaces/IMathU128.compact",
    "./IUint256": "./dist/interfaces/IUint256.compact",
    "./IMathU256": "./dist/interfaces/IMathU256.compact",
    "./IUint64": "./dist/interfaces/IUint64.compact",
    "./IMathU64": "./dist/interfaces/IMathU64.compact",
    "./IMath": "./dist/interfaces/IMath.compact",
    "./MathU128": "./dist/MathU128.compact",
    "./MathU256": "./dist/MathU256.compact",
    "./MathU64": "./dist/MathU64.compact",
    "./Uint128": "./dist/Uint128.compact",
    "./Uint256": "./dist/Uint256.compact",
    "./Uint64": "./dist/Uint64.compact",
    "./Field254": "./dist/Field254.compact",
    "./Bytes32": "./dist/Bytes32.compact",
    "./Max": "./dist/Max.compact"
  },
  "scripts": {
    "compact": "pnpm exec compact-compiler",
    "compact:fast": "pnpm exec compact-compiler --skip-zk",
    "build": "pnpm exec compact-builder && tsc",
    "test": "vitest run --printConsoleTrace",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "types": "tsc -p tsconfig.json --noEmit",
    "fmt": "biome format --write",
    "fmt:check": "biome format --check",
    "lint": "biome lint",
    "lint:fix": "biome check --write",
    "precommit": "lint-staged --no-stash && pnpm run types",
    "clean": "rm -rf dist .compact-build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.4.1",
  "devDependencies": {
    "@types/node": "^22.15.29",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@midnight-dapps/biome-config": "workspace:^",
    "@midnight-dapps/compact": "workspace:^",
    "@midnight-ntwrk/compact-runtime": "^0.8.1",
    "@midnight-ntwrk/midnight-js-network-id": "^2.0.1",
    "@midnight-ntwrk/zswap": "^4.0.0"
  }
}
