pragma language_version >= 0.14.0;

import CompactStandardLibrary;

import "../../interface/IMath";

import "../../Math" prefix Math_;

// Export IMath interface
export {
    DivResult 
};

// Export Circuits 
export {
    Math_initialize,
    Math_add,
    Math_sub,
    Math_mul,
    Math_min,
    Math_max
}

// Circuits that are leaking private output
export circuit div(
    dividend: Uint<128>,
    divisor: Uint<128>
): Uint<128> {
    return disclose(Math_div(dividend, divisor));
}

export circuit rem(
    dividend: Uint<128>,
    divisor: Uint<128>
): Uint<128> {
    return disclose(Math_rem(dividend, divisor));
}

export circuit sqrt(
    radical: Uint<128>
): Uint<128> {
    return disclose(Math_sqrt(radical));
}

export circuit isMultiple(
    value: Uint<128>,
    divisor: Uint<128>
): Boolean {
    return disclose(Math_isMultiple(value, divisor));
}
