pragma language_version >= 0.14.0;

/**
 * @description An interface module for Uint<256> types using U256 struct.
 */
module IUint256 {    
    export struct U256 {
        low: Uint<128>,
        high: Uint<128>
    }

    /** @description Constant for 2^128, used for bit decomposition of Uint<256>. */
    export ledger TWO_POW_128: Uint<256>;
}
