{
  "name": "@midnight-dapps/access",
  "version": "1.0.0-alpha.1",
  "private": true,
  "description": "Compacts for implementing LunarSwap DEX",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./artifacts/*": "./dist/artifacts/*",
    "./keys/*": "./dist/artifacts/keys/*",
    "./zkir/*": "./dist/artifacts/zkir/*"
  },
  "scripts": {
    "compact": "pnpm run clean:artifacts && pnpm exec compact-compiler",
    "build": "pnpm run clean && pnpm exec compact-builder && tsc",
    "test": "vitest run --printConsoleTrace",
    "types": "tsc -p tsconfig.json --noEmit",
    "fmt": "biome format --write",
    "lint": "biome lint",
    "lint:fix": "biome check --write",
    "clean": "pnpm run clean:dist && pnpm run clean:artifacts",
    "clean:dist": "rm -rf dist",
    "clean:artifacts": "rm -rf src/artifacts",
    "precommit": "lint-staged --no-stash && pnpm run types"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.4.1",
  "devDependencies": {
    "@types/node": "^22.15.29",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@midnight-dapps/biome-config": "workspace:^",
    "@midnight-dapps/compact": "workspace:^",
    "@midnight-dapps/compact-std": "workspace:^",
    "@midnight-dapps/structs": "workspace:^",
    "@midnight-ntwrk/compact-runtime": "^0.8.1",
    "@midnight-ntwrk/midnight-js-network-id": "^2.0.1",
    "@midnight-ntwrk/zswap": "^4.0.0"
  }
}
