{
  "name": "@openzeppelin/midnight-apps-lunarswap-cli",
  "version": "1.0.0-alpha.1",
  "private": true,
  "type": "module",
  "scripts": {
    "standalone": "node --experimental-specifier-resolution=node --loader ./src/path-resolver-loader.mjs src/launcher/standalone.ts",
    "testnet-local": "node --experimental-specifier-resolution=node --loader ./src/path-resolver-loader.mjs src/launcher/testnet-local.ts",
    "testnet-remote": "node --experimental-specifier-resolution=node --loader ./src/path-resolver-loader.mjs src/launcher/testnet-remote.ts",
    "preview-remote": "node --experimental-specifier-resolution=node --loader ./src/path-resolver-loader.mjs src/launcher/preview.ts",
    "preprod": "node --experimental-specifier-resolution=node --loader ./src/path-resolver-loader.mjs src/launcher/preprod.ts",
    "infra:up": "docker-compose -f standalone.yml up -d",
    "infra:down": "docker-compose -f standalone.yml down",
    "infra:logs": "docker-compose -f standalone.yml logs -f",
    "infra:ps": "docker-compose -f standalone.yml ps",
    "proof-server:local": "docker-compose -f proof-server-local.yml up -d",
    "proof-server:testnet": "docker-compose -f proof-server-testnet.yml up -d",
    "proof-server:down": "docker-compose -f proof-server-local.yml down && docker-compose -f proof-server-testnet.yml down",
    "build": "rm -rf dist && tsc --project tsconfig.build.json",
    "fmt": "biome format --write",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "lint:ci": "biome ci . --changed --since=main --no-errors-on-unmatched",
    "precommit": "lint-staged && pnpm run types",
    "types": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "axios": "^1.12.0",
    "@midnight-ntwrk/compact-runtime": "0.14.0",
    "@midnight-ntwrk/testkit-js": "3.0.0",
    "@midnight-ntwrk/ledger-v7": "7.0.0",
    "@midnight-ntwrk/midnight-js-contracts": "3.0.0",
    "@midnight-ntwrk/midnight-js-http-client-proof-provider": "3.0.0",
    "@midnight-ntwrk/midnight-js-indexer-public-data-provider": "3.0.0",
    "@midnight-ntwrk/midnight-js-level-private-state-provider": "3.0.0",
    "@midnight-ntwrk/midnight-js-network-id": "3.1.0",
    "@midnight-ntwrk/midnight-js-node-zk-config-provider": "3.0.0",
    "@midnight-ntwrk/midnight-js-types": "3.0.0",
    "@midnight-ntwrk/midnight-js-utils": "3.0.0",
    "@midnight-ntwrk/wallet-sdk-address-format": "^3.0.0",
    "@midnight-ntwrk/wallet-sdk-dust-wallet": "1.0.0",
    "@midnight-ntwrk/wallet-sdk-facade": "1.0.0",
    "@midnight-ntwrk/wallet-sdk-hd": "^3.0.0",
    "@midnight-ntwrk/wallet-sdk-indexer-client": "1.0.0",
    "@midnight-ntwrk/wallet-sdk-node-client": "1.0.0",
    "@midnight-ntwrk/wallet-sdk-prover-client": "1.0.0",
    "@midnight-ntwrk/wallet-sdk-runtime": "^1.0.0",
    "@midnight-ntwrk/wallet-sdk-shielded": "1.0.0",
    "@midnight-ntwrk/wallet-sdk-unshielded-wallet": "1.0.0",
    "@midnight-ntwrk/zswap": "^4.0.0",
    "@openzeppelin/midnight-apps-biome-config": "workspace:*",
    "@openzeppelin/midnight-apps-contracts": "workspace:*",
    "@openzeppelin/midnight-apps-lunarswap-api": "workspace:*",
    "@openzeppelin/midnight-networks": "workspace:*",
    "@scure/bip39": "^1.2.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "rxjs": "^7.8.1",
    "testcontainers": "^10.28.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.7",
    "@types/ws": "^8.5.10",
    "eslint": "^9.30.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
