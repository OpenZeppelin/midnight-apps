{
  "name": "@openzeppelin/midnight-apps-lunarswap-cli",
  "version": "1.0.0-alpha.1",
  "private": true,
  "type": "module",
  "scripts": {
    "testnet-local": "node --experimental-specifier-resolution=node --loader ts-node/esm src/testnet-local.ts",
    "testnet-remote": "node --experimental-specifier-resolution=node --loader ts-node/esm src/testnet-remote.ts",
    "testnet-remote-ps": "cp -r proof-server-testnet.yml ./dist/ && node --experimental-specifier-resolution=node --loader ts-node/esm src/testnet-remote-ps.ts",
    "infra:up": "docker-compose -f standalone.yml up -d",
    "infra:down": "docker-compose -f standalone.yml down",
    "infra:logs": "docker-compose -f standalone.yml logs -f",
    "infra:ps": "docker-compose -f standalone.yml ps",
    "proof-server:local": "docker-compose -f proof-server-local.yml up -d",
    "proof-server:testnet": "docker-compose -f proof-server-testnet.yml up -d",
    "proof-server:down": "docker-compose -f proof-server-local.yml down && docker-compose -f proof-server-testnet.yml down",
    "build": "rm -rf dist && tsc --project tsconfig.build.json",
    "fmt": "biome format --write",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "precommit": "lint-staged && pnpm run types",
    "types": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@openzeppelin/midnight-apps-biome-config": "workspace:*",
    "@openzeppelin/midnight-apps-lunarswap-api": "workspace:*",
    "@openzeppelin/midnight-apps-contracts": "workspace:*",
    "@midnight-ntwrk/compact-runtime": "0.14.0",
    "@midnight-ntwrk/ledger-v7": "7.0.1",
    "@midnight-ntwrk/midnight-js-contracts": "3.0.0",
    "@midnight-ntwrk/midnight-js-http-client-proof-provider": "3.0.0",
    "@midnight-ntwrk/midnight-js-indexer-public-data-provider": "3.0.0",
    "@midnight-ntwrk/midnight-js-level-private-state-provider": "3.0.0",
    "@midnight-ntwrk/midnight-js-network-id": "3.0.0",
    "@midnight-ntwrk/midnight-js-node-zk-config-provider": "3.0.0",
    "@midnight-ntwrk/midnight-js-types": "3.0.0",
    "@midnight-ntwrk/midnight-js-utils": "3.0.0",
    "@midnight-ntwrk/wallet": "^5.0.0",
    "@midnight-ntwrk/wallet-api": "^5.0.0",
    "@midnight-ntwrk/wallet-sdk-address-format": "^2.0.0",
    "@midnight-ntwrk/wallet-sdk-hd": "^2.0.0",
    "@midnight-ntwrk/zswap": "^4.0.0",
    "@scure/bip39": "^1.2.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "rxjs": "^7.8.1",
    "testcontainers": "^10.28.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.7",
    "@types/ws": "^8.5.10",
    "eslint": "^9.30.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
