    "@midnight-ntwrk/midnight-js-types": "3.0.0",
    "pino": "^9.7.0",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@openzeppelin/midnight-apps-biome-config": "workspace:*",
    "@types/node": "^24.0.7",
    "eslint": "^9.30.0",
    "typescript": "^5.8.3"
  }
}
